    <style>
        .user-item {
            width: 250px;
            padding: 0;
            font-size: 20px;
        }

        .table-responsive {
            overflow: scroll;
        }

        .input-validation-error {
            border: 1px solid #ff0000;
            background-color: #ffeeee;
        }

        .field-validation-error {
            color: red;
        }
    </style>

</head>
